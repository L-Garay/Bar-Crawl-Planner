{
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "concurrently": "^7.6.0",
    "jest": "^29.4.3",
    "jest-mock-extended": "^3.0.1",
    "parcel": "^2.8.1",
    "ts-jest": "^29.0.5"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40"
  },
  "scripts": {
    "serverUnitTests": "cd server && npm run unitTests",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && npm run generateAndCompile && npm run start & sleep 10 && curl http://localhost:3000",
    "start:client": "cd client && npm run build && npm run start & sleep 10 && curl http://localhost:3000",
    "cypress:run": "cd client && npm run cypress:run",
    "install:client": "cd client && npm install"
  },
  "engines": {
    "node": "^16.19.1"
  }
}
